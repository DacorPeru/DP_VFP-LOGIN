	KEY ALT+I, ""
DEFINE PAD _6zj10kwrw OF (m.cMenuName) PROMPT "Configuración" COLOR SCHEME 3 ;
	KEY ALT+C, ""
DEFINE PAD _6zj10kwrx OF (m.cMenuName) PROMPT "Utilitarios" COLOR SCHEME 3 ;
	KEY ALT+U, ""
